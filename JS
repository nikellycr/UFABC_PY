let idade = prompt("Qaul é sua idade? ");
let salario = prompt("Qual seu salário? ");
let valorEmprestimo = prompt("Valor do emprestimo? ");
let emp = prompt("Qauntidade de parcelas? ")

if ((21>=idade>=55) & (valorEmprestimo>= 5*salario) & (emp<=18)){
    alert("Emprestimo aprovado!")
}
else{
    alert("Emsprestimo negado!")
}
